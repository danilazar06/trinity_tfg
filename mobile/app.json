{
  "expo": {
    "name": "Trinity",
    "slug": "trinity",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "scheme": "trinity",
    "icon": "./assets/logo-trinity-v1.png",
    "splash": {
      "image": "./assets/logo-trinity-v1.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D0D0F"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trinity.app"
    },
    "android": {
      "package": "com.trinity.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.320120465080-4lf6l426q4ct2jn4mpgte9m5mbmlss7j"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "googleServicesFile": "./google-services.json"
          },
          "ios": {
            "googleServicesFile": "./GoogleService-Info.plist"
          }
        }
      ]
    ],
    "extra": {
      "googleWebClientId": "320120465080-4lf6l426q4ct2jn4mpgte9m5mbmlss7j.apps.googleusercontent.com",
      "googleIosClientId": "320120465080-4lf6l426q4ct2jn4mpgte9m5mbmlss7j.apps.googleusercontent.com",
      "googleAndroidClientId": "320120465080-4lf6l426q4ct2jn4mpgte9m5mbmlss7j.apps.googleusercontent.com",
      "eas": {
        "projectId": "trinity-voting-app"
      }
    }
  }
}
