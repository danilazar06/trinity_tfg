{
  "name": "trinity-mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:google-signin": "jest --testPathPattern=google",
    "test:properties": "jest --testPathPattern=properties",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:master": "ts-node src/tests/run-google-signin-tests.ts",
    "build:development": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:production": "eas build --profile production",
    "build:dev:android": "eas build --profile development --platform android",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:dev:all": "eas build --profile development --platform all",
    "install:dev:android": "./scripts/install-development-build.sh android",
    "install:dev:ios": "./scripts/install-development-build.sh ios",
    "dev:start": "expo start --dev-client",
    "dev:android": "expo start --dev-client --android",
    "dev:ios": "expo start --dev-client --ios",
    "diagnose:google-signin": "node scripts/diagnose-google-signin.js",
    "validate:google-signin": "node scripts/validate-google-signin.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-google-signin/google-signin": "^16.1.1",
    "expo": "~52.0.0",
    "expo-asset": "^12.0.12",
    "expo-auth-session": "~6.0.4",
    "expo-build-properties": "~0.13.2",
    "expo-constants": "~17.0.8",
    "expo-device": "~7.0.1",
    "expo-font": "~13.0.1",
    "expo-image": "~2.0.7",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.3",
    "expo-router": "~4.0.11",
    "expo-splash-screen": "^31.0.13",
    "expo-status-bar": "~2.0.0",
    "expo-web-browser": "~14.0.2",
    "react": "18.3.1",
    "react-native": "0.76.3",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.8.1",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.3.12",
    "fast-check": "^3.22.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.0"
  },
  "private": true
}
